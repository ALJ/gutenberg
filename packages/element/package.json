{
	"name": "@wordpress/element",
	"version": "4.0.4",
	"description": "Element React module for WordPress.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"gutenberg",
		"element",
		"react"
	],
	"homepage": "https://github.com/WordPress/gutenberg/tree/HEAD/packages/element/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/gutenberg.git",
		"directory": "packages/element"
	},
	"bugs": {
		"url": "https://github.com/WordPress/gutenberg/issues"
	},
	"engines": {
		"node": ">=12"
	},
	"main": "build/index.js",
	"module": "build-module/index.js",
	"react-native": "src/index",
	"types": "build-types",
	"sideEffects": false,
	"dependencies": {
		"@types/react": "^16.9.0",
		"@types/react-dom": "^16.9.0",
		"@wordpress/escape-html": "file:../escape-html",
		"lodash": "^4.17.21",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.53",
		"@swc/core": "^1.2.119",
		"@wordpress/browserslist-config": "file:../browserslist-config",
		"chokidar": "^3.5.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "npm run build:es6 -- -w",
		"build": "npm run build:cjs && npm run build:es6",
		"build:cjs": "npx swc src -d build -C module.type=commonjs --ignore **/test/**/*.js -s",
		"build:es6": "npx swc src -d build-module -C module.type=es6 --ignore **/test/**/*.js -s"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	]
}
