{
	"name": "@wordpress/a11y",
	"version": "3.2.4",
	"description": "Accessibility (a11y) utilities for WordPress.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"gutenberg",
		"accessibility",
		"a11y",
		"aria-live"
	],
	"homepage": "https://github.com/WordPress/gutenberg/tree/HEAD/packages/a11y/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/gutenberg.git",
		"directory": "packages/a11y"
	},
	"bugs": {
		"url": "https://github.com/WordPress/gutenberg/issues"
	},
	"engines": {
		"node": ">=12"
	},
	"main": "build/index.js",
	"module": "build-module/index.js",
	"react-native": "src/index",
	"types": "build-types",
	"dependencies": {
		"@wordpress/dom-ready": "file:../dom-ready",
		"@wordpress/i18n": "file:../i18n"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.53",
		"@swc/core": "^1.2.119",
		"@wordpress/browserslist-config": "file:../browserslist-config",
		"chokidar": "^3.5.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "npm run build:es6 -- -w",
		"build": "npm run build:cjs && npm run build:es6",
		"build:cjs": "npx swc src -d build -C module.type=commonjs --ignore **/test/**/*.js -s",
		"build:es6": "npx swc src -d build-module -C module.type=es6 --ignore **/test/**/*.js -s"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	]
}
