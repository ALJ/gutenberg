{
	"name": "@wordpress/i18n",
	"version": "4.2.4",
	"description": "WordPress internationalization (i18n) library.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"gutenberg",
		"i18n"
	],
	"homepage": "https://github.com/WordPress/gutenberg/tree/HEAD/packages/i18n/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/gutenberg.git",
		"directory": "packages/i18n"
	},
	"bugs": {
		"url": "https://github.com/WordPress/gutenberg/issues"
	},
	"engines": {
		"node": ">=12"
	},
	"main": "build/index.js",
	"module": "build-module/index.js",
	"react-native": "src/index",
	"types": "build-types",
	"bin": {
		"pot-to-php": "./tools/pot-to-php.js"
	},
	"dependencies": {
		"@wordpress/hooks": "file:../hooks",
		"gettext-parser": "^1.3.1",
		"lodash": "^4.17.21",
		"memize": "^1.1.0",
		"sprintf-js": "^1.1.1",
		"tannin": "^1.2.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.53",
		"@swc/core": "^1.2.119",
		"@wordpress/browserslist-config": "file:../browserslist-config",
		"chokidar": "^3.5.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "npm run build:es6 -- -w",
		"build": "npm run build:cjs && npm run build:es6",
		"build:cjs": "npx swc src -d build -C module.type=commonjs --ignore **/*.test.js -s",
		"build:es6": "npx swc src -d build-module -C module.type=es6 --ignore **/*.test.js -s"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	]
}
